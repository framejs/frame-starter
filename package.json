{
  "name": "@framejs/starter-kit",
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently \"node_modules/webpack/bin/webpack.js\" \"node_modules/@frctl/fractal/bin/fractal start --sync\"",
    "build": "node_modules/webpack/bin/webpack.js --env.production && node_modules/@frctl/fractal/bin/fractal build",
    "test": "jest --verbose"
  },
  "jest": {
    "testEnvironment": "@skatejs/ssr/jest",
    "modulePaths": [
      "dist/es2015"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": ["/node_modules/", "www"],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@framejs/core": "^0.0.5",
    "@framejs/renderer-lit-html": "^0.1.4",
    "@framejs/renderer-preact": "^0.0.5",
    "compression-webpack-plugin": "^1.1.6"
  },
  "devDependencies": {
    "@skatejs/ssr": "^0.19.1",
    "@skatejs/val": "^0.4.0",
    "autoprefixer": "^7.2.5",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^3.3.1",
    "typescript": "^2.7.1",
    "@frctl/fractal": "^1.1.7",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-visualizer-plugin": "^0.1.11",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "postcss-loader": "^2.0.10",
    "postcss-node-sass": "^2.1.4",
    "@skatejs/bore": "^4.0.3",
    "@types/jest": "^22.1.1",
    "@types/node": "^9.4.0",
    "jest": "^22.2.0",
    "ts-jest": "^22.0.3"
  }
}
